public class Day1 {
	
	int i = 4;public static void main(String[] args) { Day1 day1 = new Day1();      Day1 day2 = new Day1();
		
		day1.add();
		
	}
	
	void add() {
		
		int i = 5;
		int j = 6;
		
		System.out.println(i);
		System.out.println(this.i);
		System.out.println(j);
	}
}



