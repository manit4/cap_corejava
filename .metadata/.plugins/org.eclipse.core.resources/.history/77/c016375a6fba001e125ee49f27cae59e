
public class Day2 {

	public static void main(String[] args) {

		Day2 day2 = new Day2();

		int returnedValue = day2.add();
		
		System.out.println("The returned Value is "+returnedValue);

	}

	int add() {

		int value = 100;

		System.out.println("I am from add() " + value);

		return value;

	}

}




