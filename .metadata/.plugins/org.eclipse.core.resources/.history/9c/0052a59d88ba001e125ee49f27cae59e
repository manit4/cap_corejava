class Car {
	
	void tyres() {
		System.out.println("inside tyres() of Car");
	}
}

class Swift {
	
	void steeringWheel() {
		System.out.println("inside steeringWheel() of Swift");
	}
}

public class Day2 {

	public static void main(String[] args) {
		
		Swift swift = new Swift();   swift.steeringWheel();       swift.tyres();
		Car car = new Car();    car.tyres();
	}
}



