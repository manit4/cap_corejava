
public class Day1 {
	
	int firstNo = 4;
	int secondNo = 5;
	int result;//here, since result is not assigned any value then '0' will be assigned...
	
	
	public static void main(String[] args) {
		
		Day1 tripti = new Day1();
		
		tripti.result = tripti.firstNo + tripti.secondNo;
		
		
		System.out.println( tripti.result );
	}

}



