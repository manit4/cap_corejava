
public class Day1 {
	
	int firstNo = 4;
	int secondNo = 5;
	int result = firstNo + secondNo;
	
	public static void main(String[] args) {
		
		Day1 day1 = new Day1();
		
		
		System.out.println( day1.result );
	}

}



