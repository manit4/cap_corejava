
public class Day2 {

	public static void main(String[] args) {

		int arr[][] = { { 1, 1, 1 }, { 2, 2, 2 }, { 3, 3, 3 } };

		int[][] newArr = new int[3][3];

		for (int i = 0; i < arr.length; i++) {

			for (int j = 0; j < arr.length; j++) {

				newArr[j][i] = arr[i][j];
			}
		}

		for (int i = 0; i < arr.length; i++) {

			for (int j = 0; j < arr.length; j++) {

				System.out.print(newArr[i][j]);	
			}
			System.out.println();
			
		}
	}
}

