
public class Day2 {
	
	int firstNo = 4, secondNo = 5;
	static int result;

	public static void main(String[] args) {

		Day2 obj1 = new Day2();  Day2 obj2 = new Day2();
		
		obj1.add();
		obj2.add();
		
		System.out.println(result);

	}
	
	void add() {
		
		int result = 50;
		
		result = firstNo + secondNo + result;
		System.out.println(Day2.result);
		
		
	}

}


